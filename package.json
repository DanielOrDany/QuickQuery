    "mongodb": "^3.5.2",
    "mssql": "^6.0.1",
    "mysql": "^2.17.1",
    "mysql2": "^2.1.0",
    "node-sass": "^4.14.1",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "pretty-checkbox": "^3.0.3",
    "rc-menu": "^7.5.4",
